// Purpose: Map remote IPs to geographic region metadata.
// Expected output: Public IPs with reported country/region fields.
// Flag: 27_geo_region_from_ip
let device = "ch-ops-wks02";
let startTime = datetime(2025-11-15);
let endTime = datetime(2025-12-05);
DeviceNetworkEvents
| where DeviceName == device
| where Timestamp between (startTime .. endTime)
| where RemoteIPType == "Public"
| project Timestamp, RemoteIP, RemoteIPCountry, RemoteIPCity, InitiatingProcessCommandLine
